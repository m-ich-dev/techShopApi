{
  "name": "techshopapi",
  "version": "2.0.0",
  "description": "API for techShop",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "knex": "tsx node_modules/.bin/knex --knexfile src/knexfile.ts",
    "seed": "npm run knex -- seed:run",
    "seed:run": "npm run knex -- seed:run --specific",
    "migrate": "npm run knex -- migrate:latest",
    "migrate:rollback": "npm run knex -- migrate:rollback",
    "migrate:refresh": "npm run migrate:rollback && npm run migrate"
  },
  "author": "mich",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "knex": "^3.1.0",
    "kysely": "^0.28.11",
    "mariadb": "^3.4.5",
    "mysql2": "^3.16.3",
    "pg": "^8.18.0",
    "slug": "^11.0.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/express": "^5.0.6",
    "@types/node": "^25.2.1",
    "@types/pg": "^8.16.0",
    "@types/slug": "^5.0.9",
    "@types/typescript": "^0.4.29",
    "eslint": "^9.39.2",
    "globals": "^17.3.0",
    "jiti": "^2.6.1",
    "kysely-ctl": "^0.20.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  }
}
